.container {
  padding-bottom: 45px;
}

image {
  width: 100%;
  height: 300rpx;
}

.list {
  height: 30px;
  font-size: 14px;
  text-align: left;
  width: 100%;
  margin-top: 10px;
  border-bottom: 1px solid #ccc;
}

.list-title {
  padding: 4px 10px;
  font-weight: bold;
}

.list-item{
  width: 100%;
  padding: 4px;
  font-size: 14px;
}

.item{
  height: 24px;
  line-height: 24px;
  padding: 2px 10px;
  border-bottom:1px dashed #e6e6e6;
  color: #355A83;
}

